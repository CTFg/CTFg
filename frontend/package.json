{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styletron-engine-atomic": "^1.1.1",
    "@types/styletron-react": "^5.0.3",
    "@types/styletron-standard": "^2.0.2",
    "baseui": "^9.116.2",
    "graphql": "^15.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-react": "^6.0.1",
    "typescript": "^4.1.2",
    "uniforms": "^3.5.1",
    "uniforms-bridge-graphql": "^3.5.1"
  },
  "scripts": {
    "start": "(cd ../; docker-compose up -d) && concurrently \"react-scripts start\" \"yarn generate --watch\"",
    "fakedata": "docker-compose exec ctfd bash -c \"pip install faker; python /opt/CTFd/populate.py\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "prettier -cw .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config config/codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "concurrently": "^6.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-baseui": "^9.116.2",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "source-map-explorer": "^2.5.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "baseui"
    ],
    "rules": {
      "baseui/deprecated-theme-api": "warn",
      "baseui/deprecated-component-api": "warn",
      "baseui/no-deep-imports": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost"
}
